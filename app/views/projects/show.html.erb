<h1><%=@project.name%></h1>
<p>Description: <%=@project.description%></p>
<%= image_tag(@project.photo_url.to_s) %>
<p><%=@project.authors%></p>

<%= link_to "Edit", edit_project_path(@project) %>
<br>
<%= link_to "Delete", project_path(@project), 
											method: :delete, 
											data: { confirm: "Are you sure you want to delete this project?" } %>